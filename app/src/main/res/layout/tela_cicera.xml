<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey">


    <View
        android:id="@+id/containerTop2"
        android:layout_width="match_parent"
        android:layout_height="219dp"
        android:layout_marginTop="428dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/containerTop"
        android:layout_width="match_parent"
        android:layout_height="89dp"
        android:background="@color/dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="157dp"
        android:layout_height="85dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="@id/containerTop"
        app:layout_constraintEnd_toEndOf="@+id/containerTop"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/containerTop"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="242dp"
        android:layout_marginBottom="40dp"
        android:text="Contato"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/containerTop2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="231dp"
        android:layout_height="33dp"
        android:text="Telefone : (+55) 11 94671-2055"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="228dp"
        android:layout_height="37dp"
        android:layout_marginTop="142dp"
        android:layout_marginBottom="90dp"
        android:text="Cícera"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerTop"
        app:layout_constraintVertical_bias="0.0" />

    /

    <TextView
        android:id="@+id/textView9"
        android:layout_width="337dp"
        android:layout_height="80dp"
        android:layout_marginTop="189dp"
        android:layout_marginBottom="28dp"
        android:text="Olá! Meu nome é Cícera e trabalho confeccionando objetos artesanais  feitos a mão, como brinquedos de pano, croches, bordados e etc..."
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/containerTop2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerTop"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/homecicera"
        android:layout_width="65dp"
        android:layout_height="51dp"
        android:layout_marginTop="12dp"
        android:background="#D3121212"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="@+id/containerTop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_home_24" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="272dp"
        android:layout_height="34dp"
        android:layout_marginTop="95dp"
        android:text="confira meus trabalhos abaixo :"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/containerTop2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="123dp"
        android:layout_height="204dp"
        android:layout_marginEnd="276dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerTop2"
        app:layout_constraintEnd_toEndOf="@+id/containerTop2"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="@+id/containerTop2"
        app:layout_constraintTop_toTopOf="@+id/containerTop2"
        app:layout_constraintVertical_bias="0.333"
        app:srcCompat="@drawable/trabalho_artesa2" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="138dp"
        android:layout_height="210dp"
        android:layout_marginEnd="90dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerTop2"
        app:layout_constraintEnd_toEndOf="@+id/containerTop2"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/containerTop2"
        app:srcCompat="@drawable/trabalho_artesa3" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="126dp"
        android:layout_height="204dp"
        android:layout_marginStart="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/containerTop2"
        app:layout_constraintEnd_toEndOf="@+id/containerTop2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/containerTop2"
        app:layout_constraintTop_toTopOf="@+id/containerTop2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/trabalho_artesa1" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="213dp"
        android:layout_height="126dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerTop"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/cicera" />


</androidx.constraintlayout.widget.ConstraintLayout>